        // These checks can't trigger cross origin error
        var googleTranslate = ajq("script[src*='//translate.googleusercontent.com']").length > 0;
        var html = ajq("html");
        var chromeTranslate = html.hasClass("translated-ltr") || html.hasClass("translated-rtl");
