function formValues() {
    var firstName = ajq.trim(ajq("input[name='FirstName']").val());
    var lastName = ajq.trim(ajq("input[name='LastName']").val());
    var uEmail = ajq.trim(ajq("input[name='EmailAddress']").val()) || ajq("input[name='EMailAddress']").val();
    if (uEmail && uEmail.match(/.+@.+\..+/)) {
        Evergage.setCustomField('userEmail', uEmail, Evergage.Scope.Request);
        Evergage.setCustomField('firstName', firstName, Evergage.Scope.Request);
        Evergage.setCustomField('lastName', lastName, Evergage.Scope.Request);
        return true;
    } else {
        return false;
    }
}

