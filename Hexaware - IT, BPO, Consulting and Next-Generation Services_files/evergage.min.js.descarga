Evergage.CatMap.blogView = function(data) {
    Evergage.safeClick("div.blog-title a, div.blog-link a", function(target) {
        var blog = ajq(event.target).closest("div.blog-details");
        var item = {
            type: Evergage.ItemType.Blog,
            _id: Evergage.util.getLastPathComponentWithoutExtension(blog.find("div.blog-title a").attr("href")),
            name: ajq.trim(blog.find("div.blog-title a").first().text()),
            url: blog.find("div.blog-title a").attr("href"),
            published: Evergage.CatMap.getDateFromString(blog.find("div.calander-icon").first().text()),
            tags: []
        };

        var description = ajq.trim(blog.find("div.blog-desc").first().text());
        if (description.length > 300) {
            description = description.substr(0, 297) + "...";
        }
        item.description = description;

        if (data) {
            item.categories = [data];
        }

        if (blog.find("div.profile-dtls div.profile-icon").length > 0) {
            item.tags.push({
                type: Evergage.ItemType.Tag,
                tagType: "Author",
                _id: ajq.trim(blog.find("div.profile-icon").first().text().replace(/posted by /i, ""))
            });
        }

        Evergage.viewItem(item);
    });
};

